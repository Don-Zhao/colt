<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.colt.app.dao.ItemMapper">
	<insert id="insertItemInfo" useGeneratedKeys="true" keyProperty="id" parameterType="com.colt.app.daoobj.ItemInfo">
		insert into t_item_info(name, description, price, img) values(#{name}, #{description}, #{price}, #{img})
	</insert>
</mapper>